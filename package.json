{
  "name": "generator-yogo",
  "version": "0.1.0",
  "description":
    "Generator golang allows to create projects like (Console ('Hello world'), REST-API)",
  "homepage": "http://homepageURL.com",
  "author": {
    "name": "Andriy Lesch",
    "email": "clasikas@gmail.com",
    "url": "Andriy Lesch"
  },
  "files": ["generators"],
  "main": "generators/app/index.js",
  "keywords": [
    "yeoman-generator",
    "go",
    "golang",
    "app",
    "scaffold",
    "yogo",
    "generator-yogo"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "nsp": "^2.6.3",
    "eslint": "^4.1.0",
    "prettier": "^1.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-config-xo": "^0.18.2",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "toml": "^2.3.3",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "git@github.com:andriylesch/generator-yogo.git",
  "license": "MIT"
}
